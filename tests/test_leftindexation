mint a = {{0,1,2},{3,4,5},{6,7,8}}
print(a)

a(1,1) <- 100
print(a)

a(0, []) <- {101,102,103}
print(a)

a([], 0) <- {201,202,203}
print(a)

a({0,1}, []) <- {{11,22},{33,44}, {55,66}}
print(a)

a([], {0,1}) <- {{101,102,103},{104,105,106}}
print(a)

a({true, false, true}, []) <- {{90,91},{92,93}, {94,95}}
print(a)

a([], {false, true, true}) <- {{200,201,202},{207,208,209}}
print(a)

a({{true, true, false},{false, true, true},{true, false, true}}) <- {{1000, 1500},{2000, 2500},{3000, 3500}}
print(a)
